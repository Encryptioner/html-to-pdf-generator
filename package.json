{
  "name": "@encryptioner/html-to-pdf-generator",
  "version": "5.0.0",
  "description": "Modern multi-page PDF generator from HTML content with smart pagination and styling support",
  "keywords": [
    "pdf",
    "pdf-generator",
    "html-to-pdf",
    "jspdf",
    "html2canvas-pro",
    "oklch",
    "react",
    "vue",
    "svelte",
    "typescript",
    "multi-page",
    "pagination",
    "document-generator",
    "framework-agnostic"
  ],
  "author": {
    "name": "Mir Mursalin Ankur",
    "email": "mir.ankur.ruet13@gmail.com",
    "url": "https://encryptioner.github.io/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/encryptioner/html-to-pdf-generator.git"
  },
  "bugs": {
    "url": "https://github.com/encryptioner/html-to-pdf-generator/issues"
  },
  "homepage": "https://github.com/encryptioner/html-to-pdf-generator#readme",
  "packageManager": "pnpm@9.0.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "react": [
        "./dist/react.d.ts"
      ],
      "vue": [
        "./dist/vue.d.ts"
      ],
      "svelte": [
        "./dist/svelte.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.js",
      "require": "./dist/react.cjs"
    },
    "./vue": {
      "types": "./dist/vue.d.ts",
      "import": "./dist/vue.js",
      "require": "./dist/vue.cjs"
    },
    "./svelte": {
      "types": "./dist/svelte.d.ts",
      "import": "./dist/svelte.js",
      "require": "./dist/svelte.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build && pnpm run typecheck"
  },
  "dependencies": {
    "html2canvas-pro": "^1.5.8",
    "jspdf": "^2.5.2"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "svelte": "^4.0.0 || ^5.0.0",
    "vue": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "svelte": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "svelte": "^4.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0",
    "vue": "^3.0.0"
  },
  "engines": {
    "node": ">=18.20.0",
    "pnpm": ">=8.0.0"
  }
}
